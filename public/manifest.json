{
  "name": "Freelancer Projects",
  "description": "This extension will help you to find projects on freelancer.com and upwork.com",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_title": "Open the popup"
  },
  "background": {
    "service_worker": "./static/js/background.js"
  },
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "permissions": ["storage", "activeTab", "alarms", "notifications"]
}
